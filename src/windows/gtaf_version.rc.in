#include <winver.h>

VS_VERSION_INFO VERSIONINFO
 FILEVERSION     @GTAF_VERSION_COMMA@
 PRODUCTVERSION  @GTAF_VERSION_COMMA@
 FILEFLAGSMASK   VS_FFI_FILEFLAGSMASK
 FILEFLAGS       0
 FILEOS          VOS_NT_WINDOWS32
 FILETYPE        VFT_APP
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName",      "GTAF"
      VALUE "FileDescription",  "GTAF Example"
      VALUE "FileVersion",      "@GTAF_FULL_VERSION@"
      VALUE "ProductName",      "GTAF Framework"
      VALUE "ProductVersion",   "@GTAF_FULL_VERSION@"
      VALUE "OriginalFilename", "gtaf_example.exe"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1200
  END
END
